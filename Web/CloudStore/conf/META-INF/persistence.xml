<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="cloudviewPersistenceUnit" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>com.cloudview.model.Animal</class>
		<class>com.cloudview.model.AnimalFile</class>
		<class>com.cloudview.model.Bucket</class>
		<class>com.cloudview.model.Desouscription</class>
		<class>com.cloudview.model.Diagnosi</class>
		<class>com.cloudview.model.Examination</class>
		<class>com.cloudview.model.Image</class>
		<class>com.cloudview.model.ImageSerie</class>
		<class>com.cloudview.model.ImageSharedWith</class>
		<class>com.cloudview.model.Notification</class>
		<class>com.cloudview.model.Owner</class>
		<class>com.cloudview.model.OwnerHasAnimal</class>
		<class>com.cloudview.model.ServiceNotification</class>
		<class>com.cloudview.model.Souscription</class>
		<class>com.cloudview.model.Study</class>
		<class>com.cloudview.model.Surgery</class>
		<class>com.cloudview.model.User</class>
		<class>com.cloudview.model.Veterinary</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/cloudviewRemote"/>
			<property name="javax.persistence.jdbc.user" value="postgres"/>
			<property name="javax.persistence.jdbc.password" value="tp"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
