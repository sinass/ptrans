<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="cloudviewPersistenceUnit" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>com.cloudview.model.AppAdmin</class>
		<class>com.cloudview.model.AppAnimal</class>
		<class>com.cloudview.model.AppAnimalFile</class>
		<class>com.cloudview.model.AppBucket</class>
		<class>com.cloudview.model.AppConfig</class>
		<class>com.cloudview.model.AppExamination</class>
		<class>com.cloudview.model.AppImage</class>
		<class>com.cloudview.model.AppImageSerie</class>
		<class>com.cloudview.model.AppImageSharedWith</class>
		<class>com.cloudview.model.AppNotification</class>
		<class>com.cloudview.model.AppOwner</class>
		<class>com.cloudview.model.AppOwnerHasAnimal</class>
		<class>com.cloudview.model.AppResilliation</class>
		<class>com.cloudview.model.AppServiceNotification</class>
		<class>com.cloudview.model.AppSouscription</class>
		<class>com.cloudview.model.AppStudy</class>
		<class>com.cloudview.model.AppSurgery</class>
		<class>com.cloudview.model.AppUser</class>
		<class>com.cloudview.model.AppVeterinary</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/remote"/>
			<property name="javax.persistence.jdbc.user" value="postgres"/>
			<property name="javax.persistence.jdbc.password" value="tp"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
